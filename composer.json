{
    "name": "coderfleek/sso-client",
    "description": "CoderFleek SSO Client for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CoderFleek",
            "email": "admin@coderfleek.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CoderFleek\\SSO\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoderFleek\\SSO\\SsoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
